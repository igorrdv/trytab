generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  createdAt DateTime @default(now())
}

model Job {
  id String @id @default(cuid())
  title String
  description String
  company String
  location String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}